{
    "presets": [
        ["env", {
            "modules": false
        }],
        "react",
        // for arrow function
        "stage-0"
    ],
    "env": {
        "development": {
            "plugins": ["react-hot-loader/babel"]
        }
    },
    "plugins": [
        // for async/await
        ["transform-runtime", {
            "helpers": false,
            "polyfill": false,
            "regenerator": true,
            "moduleName": "babel-runtime"
        }],
    ]
}
